{
	"compilerOptions": {
		"target": "es2022",
		"module": "esnext",
		"lib": [
			"esnext"
		],
		"moduleResolution": "bundler",
		"types": [
			"@cloudflare/workers-types"
		],
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}